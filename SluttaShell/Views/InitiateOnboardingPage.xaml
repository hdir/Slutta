    Shell.NavBarIsVisible="False"
    NavigationPage.HasNavigationBar="false"
    BackgroundColor="#f3f3f3"
    x:Name="OnboardingCarousel"
    >
    <ContentPage
        AbsoluteLayout.LayoutBounds="0, 0, 100, 100"
        AbsoluteLayout.LayoutFlags="SizeProportional"
        BackgroundColor="#f3f3f3"
        >
        <StackLayout
            Padding="25, 80, 25, 25"
            AbsoluteLayout.LayoutBounds="0, 0, 100, 100"
            AbsoluteLayout.LayoutFlags="SizeProportional"
            BackgroundColor="#f3f3f3"
            >

            <StackLayout.Background>
                <!-- StartPoint defaults to (0,0) -->
                <LinearGradientBrush EndPoint="0,1">
                    <GradientStop Color="#FAF5F5"
                                    Offset="0.1" />
                    <GradientStop Color="#E6E1E1"
                                    Offset="1.0" />
                </LinearGradientBrush>
            </StackLayout.Background>
            
            <Image Source="onboardingintro" VerticalOptions="Fill"></Image>
            <Label Text="Hva vil du slutte med?" HorizontalOptions="Center" TextColor="#082329" Padding="10, 20, 10, 10" FontSize="28" FontFamily="{StaticResource Slutta-Medium}"></Label>
            <Label Text="Appen er din personlige motivator." HorizontalOptions="Center" TextColor="#082329" Margin="10, 10, 10, 0" FontFamily="{StaticResource Roboto-Regular}"></Label>
            <Label Text="Å bli tobakksfri er lettere enn du tror." HorizontalOptions="Center" TextColor="#082329" Margin="10, 0, 10, 20" FontFamily="{StaticResource Roboto-Regular}"></Label>

            <Grid Padding="0" VerticalOptions="Start">
                <Grid.RowDefinitions>
                    <RowDefinition Height="1" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="1" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="70*" />
                    <ColumnDefinition Width="30*" />
                </Grid.ColumnDefinitions>
                <BoxView Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2"
                        BackgroundColor="#C6CBCC" HeightRequest="1"
                        VerticalOptions="End" HorizontalOptions="FillAndExpand"/>
                <Label  Text="Jeg vil slutte å røyke"
                        Padding="0,10,0,10"
                        FontSize="18" FontFamily="{StaticResource Roboto-Medium}"
                        Grid.Row="1" Grid.Column="0"
                        VerticalOptions="Center" />
                <Switch Grid.Row="1" Grid.Column="1" HorizontalOptions="End"
                        VerticalOptions="Center"
                        IsToggled="{Binding StoffList[0].IsActive}"
                        OnColor="#1A5C6A"
                        x:Name="royk_active"
                        />

                <BoxView Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2"
                        BackgroundColor="#C6CBCC" HeightRequest="1"
                        VerticalOptions="End" HorizontalOptions="FillAndExpand"/>
                <Label Text="Jeg vil slutte å snuse"
                       Padding="0,10,0,10"
                        FontSize="18" FontFamily="{StaticResource Roboto-Medium}"
                        Grid.Row="3" Grid.Column="0"
                        VerticalOptions="Center" />
                <Switch Grid.Row="3" Grid.Column="1"
                        IsToggled="{Binding StoffList[1].IsActive}"
                        HorizontalOptions="End"
                        VerticalOptions="Center" 
                        OnColor="#1A5C6A"
                        x:Name="snus_active"
                        />
            </Grid>
            <Button x:Name="NextBtn"
                    Text="Neste"
                    VerticalOptions="CenterAndExpand"
                    HorizontalOptions="Fill"
                    TextColor="#ffffff" FontSize="24"
                    FontFamily="{StaticResource Roboto-Regular}"
                    BackgroundColor="#CBCBCB"
                    CornerRadius="0"
                    Padding="15"
                    Clicked="SaveActivePreferences" />
<!--EF3340 -->
